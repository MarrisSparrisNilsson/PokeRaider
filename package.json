    "bugs": {
        "url": "https://github.com/MarrisSparrisNilsson/PokeRaider/issues"
    },
    "homepage": "https://github.com/MarrisSparrisNilsson/PokeRaider#readme",
    "dependencies": {
        "concurrently": "^5.2.0",
        "express": "^4.17.1",
        "moment": "^2.27.0",
        "mongoose": "^5.9.21",
        "socket.io": "^2.4.0"
    },
    "devDependencies": {
        "node-sass": "^4.14.1",
        "nodemon": "^2.0.4"
    }
}
